#!nsh
#
# @name UASys Bravo Mini
#
# @type Quadrotor Wide
#
# @output MAIN1 motor 1
# @output MAIN2 motor 2
# @output MAIN3 motor 3
# @output MAIN4 motor 4
#
# @maintainer Kabir Mohammed <kabir@uasys.io>
#

sh /etc/init.d/rc.mc_defaults

if [ $AUTOCNF == yes ]
then

	# Battery and Mauch Dual Power Module parameters
	param set BAT_N_CELLS 6
	param set BAT_V_EMPTY 3.5
	param set BAT_A_PER_V 65.53557
	param set BAT_V_DIV 19.0836

	# Multicopter Controller parameters
	param set MC_ROLL_P 5.0
	param set MC_ROLLRATE_P 0.05
	param set MC_ROLLRATE_I 0.02
	param set MC_ROLLRATE_D 0.0

	param set MC_PITCH_P 5.0
	param set MC_PITCHRATE_P 0.05
	param set MC_PITCHRATE_I 0.02
	param set MC_PITCHRATE_D 0.0

	param set MC_YAW_P 2.5
	param set MC_YAWRATE_P 0.2
	param set MC_YAWRATE_I 0.1
	param set MC_YAWRATE_D 0.0
	param set MC_YAWRATE_MAX 100

	# Specific to the DJI E800 Propulsion System
	param set PWM_MIN 1145
	param set PWM_MAX 2025

	# Companion computer link
	param set SYS_COMPANION 921600

	# SF11/C Lidar
	param set SENS_EN_SF1XX 4

	# Pixhawk 2.1 rotation
	param set SENS_BOARD_ROT 6
fi

set MIXER quad_w

set PWM_OUT 1234
