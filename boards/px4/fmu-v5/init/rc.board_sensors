#!/bin/sh
#
# PX4 FMUv5 specific board sensors init
#------------------------------------------------------------------------------

# External SPI bus ADIS16497
adis16497 -R 12 start

# Possible external compasses
ist8310 -C -b 1 start
ist8310 -C -b 2 start
hmc5883 -C -T -X start
qmc5883 -X start
# Rotation set for RM3100 mounted on testbed
rm3100 -R 2 -X start

# Possible internal compass
ist8310 -C -b 5 start